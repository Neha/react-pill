    "@rollup/plugin-typescript": "^12.1.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "rollup": "^4.22.5",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.6.2"
